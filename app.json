{
  "expo": {
    "name": "sgsilat",
    "slug": "sgsilat",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/silatlogo.jpeg",
    "splash": {
      "image": "./assets/silatlogo.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shubhanshusahu.sgsilat"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/silatlogo.jpeg",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.shubhanshusahu.sgsilat"
    },
    "web": {
      "favicon": "./assets/silatlogo.jpeg"
    }
  }
}
